    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregateLevel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregateLevel">
