    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregateQuantity -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregateQuantity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/AggregatedProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label>aggregate quantity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregatedOnDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregatedOnDate">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label>is aggregated on date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregatedOnHour -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/aggregatedOnHour">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>is aggregated on hour</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/hasSimpleUnit -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/hasSimpleUnit">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has simple unit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/log10Unit -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/log10Unit">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label>scale unit with log based on 10</rdfs:label>
    </owl:DatatypeProperty>
    


