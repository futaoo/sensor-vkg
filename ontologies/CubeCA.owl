        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalUnit"/>
        <rdfs:label>aggregate level</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/attachedClass -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/attachedClass">
        <rdfs:domain rdf:resource="http://purl.org/linked-data/cube#ComponentSpecification"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/madeByAggregator -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/madeByAggregator">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/AggregatedProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/Aggregator"/>
        <rdfs:label>made by aggregator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/onProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/onProperty">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jiantaowu/ontologies/2022/3/CubeCA/AggregatedProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/Property"/>
        <rdfs:label>aggregate on property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location"/>
    


